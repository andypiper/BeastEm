0052+  0039 1E 64                           LD      E, 100                  ; 100 cycles = 1/3 of a sec
0053+  003B 7A          _beep_loop          LD      A, D
0054+  003C EE 08                           XOR     AUDIO_MASK
0055+  003E D3 24                           OUT     (AUDIO_PORT), A
0056+  0040             
0057+  0040 4D                              LD      C, L
0058+  0041 44          _beep_delay0        LD      B, H
0059+  0042 10 FE       _beep_delay1        DJNZ    _beep_delay1            ; 13 * (count-2) + 8
0060+  0044 0D                              DEC     C
0061+  0045 20 FA                           JR      NZ, _beep_delay0
0062+  0047             
0063+  0047 7A                              LD      A, D
0064+  0048 D3 24                           OUT     (AUDIO_PORT), A
0065+  004A             
0066+  004A 4D                              LD      C, L
