0030+  001D             ;
0031+  001D 3E 07                           LD      A, PIO_SET_INTERRUPT    ; Ports A/B Interrupt control - no interrupts
0032+  001F D3 12                           OUT     (PIO_A_CTRL), A
0033+  0021 D3 13                           OUT     (PIO_B_CTRL), A
0034+  0023             
0035+  0023 3E CF                           LD      A, PIO_MODE_3           ; Port A Mode 3 
0036+  0025 D3 12                           OUT     (PIO_A_CTRL), A
0037+  0027 3E FF                           LD      A, 0FFh                 ; All inputs
0038+  0029 D3 12                           OUT     (PIO_A_CTRL), A
0039+  002B             
0040+  002B             
